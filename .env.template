# OpenAI API key for the agent
OPENAI_API_KEY=your_openai_api_key_here

# Source AWS account credentials (for discovery)
SRC_AWS_ACCESS_KEY_ID=your_source_aws_access_key_id
SRC_AWS_SECRET_ACCESS_KEY=your_source_aws_secret_access_key

# Destination AWS account credentials (for deployment)
DEST_AWS_ACCESS_KEY_ID=your_destination_aws_access_key_id
DEST_AWS_SECRET_ACCESS_KEY=your_destination_aws_secret_access_key

# AWS region to use (defaults to us-east-1 if not set)
AWS_REGION=us-east-1
